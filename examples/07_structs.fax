-- Example 7: Structs
-- User-defined data structures

struct Point {
    x: i32,
    y: i32
}

struct Rectangle {
    top_left: Point,
    bottom_right: Point
}

fn make_point(x: i32, y: i32) -> Point {
    Point { x: x, y: y }
}

fn point_distance(p1: Point, p2: Point) -> i32 {
    let dx = p2.x - p1.x
    let dy = p2.y - p1.y
    dx * dx + dy * dy
}

fn main() -> i32 {
    let p1 = make_point(0, 0)
    let p2 = make_point(3, 4)
    point_distance(p1, p2)
}
