-- Example 2: Arithmetic Operations
-- Demonstrates basic arithmetic

fn add(a: i32, b: i32) -> i32 { a + b }
fn sub(a: i32, b: i32) -> i32 { a - b }
fn mul(a: i32, b: i32) -> i32 { a * b }
fn div(a: i32, b: i32) -> i32 { a / b }
fn mod(a: i32, b: i32) -> i32 { a % b }

fn calculate() -> i32 {
    let x = add(10, 5)
    let y = mul(x, 2)
    let z = div(y, 3)
    mod(z, 4)
}

fn main() -> i32 {
    calculate()
}
